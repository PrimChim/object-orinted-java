package heraldcollege;

public class Mobile {
	String brand;
	
	// constructor
	Mobile(){
		brand = "nokia";
	}
	
	Mobile(String brand){
		this.brand = brand;
	}
	
	public static void main(String [] args) {
		Mobile nokia = new Mobile();
		
		System.out.println(nokia.brand);
		Mobile samsung = new Mobile("samsung");
		
		System.out.println(samsung.brand);
	}
}
