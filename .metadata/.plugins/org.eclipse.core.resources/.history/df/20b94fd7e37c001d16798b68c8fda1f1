package heraldcollege;

public class Mobile {
	String brand;

	Mobile(){  	// constructor
		brand = "nokia";
	}
	Mobile(String brand){
		this.brand = brand;
	}
	public void printBrand() { 	// methods
		System.out.println(this.brand);
	}
	final public void printBrand(String brand) { // method with final keyword
		System.out.println(brand);
	}
}

class Phone extends Mobile {
	@Override  // overrride annotation
	public void printBrand() {
		System.out.println("L.G.");
	}
	
	public static void main(String[] args) {
		Phone lg = new Phone();
		lg.printBrand();
		
		Mobile nokia = new Mobile();
		nokia.printBrand();
	}
	
}
