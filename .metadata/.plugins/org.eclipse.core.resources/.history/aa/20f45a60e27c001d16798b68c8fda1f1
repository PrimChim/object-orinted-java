package heraldcollege;

public class Mobile {
	String brand;

	Mobile(){  	// constructor
		brand = "nokia";
	}
	Mobile(String brand){
		this.brand = brand;
	}
	public void printBrand() { 	// methods
		System.out.println(this.brand);
	}
	public void printBrand(String brand) {
		System.out.println(brand);
	}
	public static void main(String [] args) { // main method
		Mobile nokia = new Mobile();
		System.out.println(nokia.brand);
		
		Mobile samsung = new Mobile("samsung");
		System.out.println(samsung.brand);
		
		nokia.printBrand();
		nokia.printBrand("Samsung");
	}
}
